    <script>
        var container = document.getElementById("map");
        var x = "37.559139240706656";
        var y = "126.98445471104401";
        var options = {
            center: new kakao.maps.LatLng(x, y),
            level: 3
        };
 
        var map = new kakao.maps.Map(container, options);
        
     	// 마커가 표시될 위치입니다 
        var markerPosition  = new kakao.maps.LatLng(x, y); 

        // 마커를 생성합니다
        var marker = new kakao.maps.Marker({
            position: markerPosition
        });

        // 마커가 지도 위에 표시되도록 설정합니다
        marker.setMap(map);
        
        // 클립보드 기능
        function copy(pp) {
        	var copyText = "";
        	switch (pp) {
			case "HA":
				copyText = document.getElementById("HA");
				break;
			case "HPA":
				copyText = document.getElementById("HPA");
				break;
			case "WA":
				copyText = document.getElementById("WA");
				break;
			case "WPA":
				copyText = document.getElementById("WPA");
				break;
			default:
				copyText = "";
				break;
			}
        	
        	if(copyText != "") {
	        	// 복사할 텍스트를 변수에 할당해줍니다.
                var text = copyText.innerText;
                // input text 태그를 생성해줍니다.
                var createInput = document.createElement("input");
                createInput.setAttribute("type", "text");
                // 가상으로 가져올 태그에 만들어준 input 태그를 붙여줍니다.
                copyText.appendChild(createInput);
                // 만든 input 태그의 value 값에 복사할 텍스트 값을 넣어줍니다.
                createInput.value = text;
                // 복사 기능을 수행한 후
                createInput.select();
                document.execCommand('copy');
                // 가상으로 붙여주었던 input 태그를 제거해줍니다.
                copyText.removeChild(createInput);
                alert('복사가 완료되었습니다.');
        	}
        }
    </script>
